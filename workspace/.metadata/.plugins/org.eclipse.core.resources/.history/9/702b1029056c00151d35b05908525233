		public void actionPerformed(ActionEvent e) {

			hoogsteGetal = Double.parseDouble(tf.getText());
num = hoogsteGetal;
		if(hoogsteGetal > res)
			res = hoogsteGetal;
		
		laagsteGetal = Double.parseDouble(tf.getText());
		res2 = Double.parseDouble(tf.getText());
		
