	<body>
		<ul>
			<li style="display: inline-block; padding: 20px 25px; list-style: none; background-color: rgb(204, 204, 204);">home</li>
			<li style="display: inline-block; padding: 20px 25px; list-style: none; background-color: rgb(204, 204, 204);">contact</li>
			<li style="display: inline-block; padding: 20px 25px; list-style: none; background-color: rgb(204, 204, 204);">producten</li>
		</ul>
	

	<script src="../../2.1.4_jquery.min.js"></script>
	<script>
		$(document).ready(function() {
			$('li').eq(0).html("home");
			$('li').eq(1).html("contact");
			$('li').eq(2).html("producten");
			
			$('li').css({
				"display": "inline-block",
				"padding": "20px 25px",
				"background-color": "#CCCCCC",
				"list-style": "none"
			});
		});
	</script>
</body></html>